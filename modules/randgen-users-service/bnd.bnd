Bundle-Name: RandGen Users Service
Bundle-SymbolicName: com.github.lgdd.liferay.randgen.users.service
Bundle-Version: 1.0.0

Import-Package:\
	!kotlin.*,\
	!org.apache.commons.io.*,\
	*

Conditional-Package:\
	org.apache.commons.io.*,\
	kotlin.*


Bundle-ClassPath:\
	.,\
    lib/httpcore-osgi.jar,\
    lib/httpclient.jar


-includeresource:\
    lib/httpcore-osgi.jar=httpcore-osgi-[0-9]*.jar,\
    lib/httpclient.jar=httpclient-[0-9]*.jar

-sources: true